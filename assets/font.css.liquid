@font-face {
    font-family: 'Jost';
    src: url("{{ 'Jost-Black.eot' | file_url }}");
    src: url("{{ 'Jost-Black.eot' | file_url }}") format('embedded-opentype'),
        url("{{ 'Jost-Black.woff2' | file_url }}") format('woff2'),
        url("{{ 'Jost-Black.woff' | file_url }}") format('woff'),
        url("{{ 'Jost-Black.ttf' | file_url }}") format('truetype'),
        url("{{ 'Jost-Black.svg' | assest_url }}") format('svg');
    font-weight: 900;
    font-style: normal;
    font-display: swap;
}

